{
	"compilerOptions": {
		"incremental": true,
		"target": "ESNext",
		"module": "ESNext",
		"declaration": true,
		"sourceMap": true,
		"composite": true,
		"strict": true,
		"moduleResolution": "NodeNext",
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true
	}
}
