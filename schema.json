{
	"title": "JSON schema for Release Drafter yaml files",
	"$id": "https://raw.githubusercontent.com/release-drafter/release-drafter/main/schema.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$ref": "#/definitions/ReleaseDrafter",
	"definitions": {
		"ReleaseDrafter": {
			"type": "object",
			"properties": {
				"references": {
					"type": "array",
					"items": { "type": "string" },
					"default": ["main"]
				},
				"change-template": {
					"type": "string",
					"default": "* $TITLE (#$NUMBER) @$AUTHOR"
				},
				"change-title-escapes": { "type": "string", "default": "" },
				"no-changes-template": { "type": "string", "default": "* No changes" },
				"version-template": {
					"type": "string",
					"default": "$MAJOR.$MINOR.$PATCH"
				},
				"name-template": { "type": "string", "default": "v$RESOLVED_VERSION" },
				"tag-prefix": { "type": "string", "default": "v" },
				"tag-template": { "type": "string", "default": "v$RESOLVED_VERSION" },
				"exclude-labels": {
					"type": "array",
					"items": { "type": "string" },
					"default": []
				},
				"include-labels": {
					"type": "array",
					"items": { "type": "string" },
					"default": []
				},
				"include-paths": {
					"type": "array",
					"items": { "type": "string" },
					"default": []
				},
				"exclude-contributors": {
					"type": "array",
					"items": { "type": "string" },
					"default": []
				},
				"no-contributors-template": {
					"type": "string",
					"default": "No contributors"
				},
				"sort-by": {
					"type": "string",
					"enum": ["merged_at", "title"],
					"default": "merged_at"
				},
				"sort-direction": {
					"type": "string",
					"enum": ["ascending", "descending"],
					"default": "descending"
				},
				"prerelease": { "type": "boolean", "default": false },
				"filter-by-commitish": { "type": "boolean", "default": false },
				"commitish": { "type": "string", "default": "" },
				"replacers": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"search": { "type": "string" },
							"replace": {
								"anyOf": [
									{ "type": "string" },
									{ "type": "string", "const": "" }
								]
							}
						},
						"required": ["search", "replace"],
						"additionalProperties": false
					},
					"default": []
				},
				"autolabeler": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"label": { "type": "string" },
							"files": {
								"type": "array",
								"items": { "type": "string" },
								"default": []
							},
							"branch": {
								"type": "array",
								"items": { "type": "string" },
								"default": []
							},
							"title": {
								"type": "array",
								"items": { "type": "string" },
								"default": []
							},
							"body": {
								"type": "array",
								"items": { "type": "string" },
								"default": []
							}
						},
						"required": ["label"],
						"additionalProperties": false
					},
					"default": []
				},
				"categories": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"title": { "type": "string" },
							"collapse-after": {
								"type": "number",
								"minimum": 0,
								"default": 0
							},
							"labels": {
								"type": "array",
								"items": { "type": "string" },
								"default": []
							}
						},
						"required": ["title"],
						"additionalProperties": false
					},
					"default": []
				},
				"version-resolver": {
					"type": "object",
					"properties": {
						"major": {
							"type": "object",
							"properties": {
								"labels": {
									"type": "array",
									"items": { "type": "string" },
									"default": []
								}
							},
							"additionalProperties": false
						},
						"minor": {
							"type": "object",
							"properties": {
								"labels": {
									"type": "array",
									"items": { "type": "string" },
									"default": []
								}
							},
							"additionalProperties": false
						},
						"patch": {
							"type": "object",
							"properties": {
								"labels": {
									"type": "array",
									"items": { "type": "string" },
									"default": []
								}
							},
							"additionalProperties": false
						},
						"default": {
							"type": "string",
							"enum": ["major", "minor", "patch"],
							"default": "patch"
						}
					},
					"required": ["major", "minor", "patch"],
					"additionalProperties": false,
					"default": {
						"major": { "labels": [] },
						"minor": { "labels": [] },
						"patch": { "labels": [] },
						"default": "patch"
					}
				},
				"category-template": { "type": "string", "default": "## $TITLE" },
				"header": { "type": "string", "default": "" },
				"template": {
					"type": "string",
					"minLength": 1,
					"default": "## Whatâ€™s Changed\n\n$CHANGES"
				},
				"footer": { "type": "string", "default": "" },
				"_extends": { "type": "string" }
			},
			"additionalProperties": false
		}
	}
}
